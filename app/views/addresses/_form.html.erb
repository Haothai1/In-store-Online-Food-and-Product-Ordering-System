<%= form_for @address do |f| %>
  <div>
    <%= f.label :street %>
    <%= f.text_field :street %>
  </div>

  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div>
    <%= f.label :zip %>
    <%= f.text_field :zip %>
  </div>

  <%= f.submit %>
<% end %>

